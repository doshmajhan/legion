  "source": "pragma solidity^0.5.0;\n\ncontract Controller {\n    address owner;\n    string public command;\n    \n    constructor() public {\n        owner = msg.sender;\n    }\n\n    function send_command() public returns (string memory) {\n        command = \"echo 'hello'\";\n        return command;\n    }\n}",
